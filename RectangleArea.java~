
/** 
*Displays the Preimeter of rectangle
*/ 




class RectanglePerimeter{             
   public static void main ( String[] args ){               
      int length = 13; //longer side of rectangle
      int width = 4;  //shorter side of rectangle
      int preimeter;    //calculated perimeter of rectangle      
      perimeter = length + length + width + width;         
      System.out.println("Prermeter of rectangle: "+ perimeter);                   
    }     
}