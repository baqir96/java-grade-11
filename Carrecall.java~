import java.util.Scanner;
public class Carrecall
{ public static void main ( String[] args ){
  int modelnumber;
  Scanner scan = new Scanner( System.in );
  System.out.println("Enter the model number of your car: ");
  modelnumber = scan.nextInt();
  if ( modelnumber == 119){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 179 ){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 189 ){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 221){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 780){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 190){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 191){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 192){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 193){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 194){
  System.out.println( "You car is defective. It must be repaired.");
  } else if ( modelnumber == 195){
  System.out.println( "You car is defective. It must be repaired.");
  } else {
  System.out.println ("Your car is not defective");
  }}
}
