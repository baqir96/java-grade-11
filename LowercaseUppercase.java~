public class LowercaseUppercase
{             public static void main ( String[] args ){    
  String str1;      
  String str2;  
  str1="apple";
  str2="APPLE";
  System.out.println(str1.toUpperCase());
  System.out.println(str2.toLowerCase());
  
  
  
}
}